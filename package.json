      "git add"
    ]
  },
  "author": {
    "name": "zenxds"
  },
  "publishConfig": {},
  "repository": {
    "type": "git",
    "url": ""
  },
  "devDependencies": {
    "@dx/hawk-cli": "^0.0.29",
    "core-js": "^3.6.5",
    "regenerator-runtime": "^0.13.7"
  },
  "dependencies": {
    "bundle-loader": "^0.5.6"
  }
}
