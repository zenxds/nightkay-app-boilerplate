{
  "name": "nightkay-app-boilerplate",
  "version": "0.1.0",
  "description": "nightkay-app-boilerplate",
  "main": "build/main.js",
  "scripts": {
    "start": "hawk start",
    "_build": "hawk build",
    "build:vendor": "hawk vendor",
    "build": "npm run _build && npm run tar",
    "tar": "hawk tar",
    "prettier": "hawk prettier"
